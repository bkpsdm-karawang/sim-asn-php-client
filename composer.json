{
    "name": "bkpsdm-karawang/sim-asn-php-client",
    "description": "PHP Client for SIM-ASN Service BKPSDM Karawang",
    "version": "2.1.1",
    "type": "library",
    "keywords": [
        "sim asn",
        "bkpsdm",
        "karawang"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denma Syarikin",
            "email": "denmasyarikin@gmail.com"
        }
    ],
    "homepage": "https://sim-asn.bkpsdm.karawangkab.go.id",
    "source": {
        "type": "git",
        "url": "https://github.com/Seldaek/monolog.git",
        "reference": "fd4380d6fc37626e2f799f29d91195040137eba9"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/pagination": "^10.0",
        "illuminate/database": "^10.0",
        "denmasyarikin/laravel-api-pagination": "^0.1"
    },
    "suggest": {
        "laravel/framework": "^9.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "SIM_ASN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SIM_ASN\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SIM_ASN\\Laravel\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix . --rules=@Symfony"
        ]
    }
}
